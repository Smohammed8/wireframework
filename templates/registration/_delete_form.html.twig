<form method="post" action="{{ path('app_registration_delete', {'id': registration.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ registration.id) }}">
    <button class="btn  btn-sm btn-outline-primary  mr-1"> <i class="fa fa-trash-alt"> </i> Delete</button>
</form>
