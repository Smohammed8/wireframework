<form method="post" action="{{ path('app_internal_experience_delete', {'id': internal_experience.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ internal_experience.id) }}">
    <button class="btn  btn-sm btn-outline-secondary  mr-1"><i class="far fa-trash-alt"></i>  Delete</button>
</form>
