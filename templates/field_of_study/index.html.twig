{% extends 'base.html.twig' %}

{% block title %}FieldOfStudy index{% endblock %}

{% block body %}
    <h1>FieldOfStudy index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for field_of_study in field_of_studies %}
            <tr>
                <td>{{ field_of_study.id }}</td>
                <td>{{ field_of_study.name }}</td>
                <td>
                    <a href="{{ path('app_field_of_study_show', {'id': field_of_study.id}) }}">show</a>
                    <a href="{{ path('app_field_of_study_edit', {'id': field_of_study.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_field_of_study_new') }}">Create new</a>
{% endblock %}
