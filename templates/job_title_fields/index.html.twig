{% extends 'base.html.twig' %}

{% block title %}JobTitleFields index{% endblock %}

{% block body %}
    <h1>JobTitleFields index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for job_title_field in job_title_fields %}
            <tr>
                <td>{{ job_title_field.id }}</td>
                <td>
                    <a href="{{ path('app_job_title_fields_show', {'id': job_title_field.id}) }}">show</a>
                    <a href="{{ path('app_job_title_fields_edit', {'id': job_title_field.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_job_title_fields_new') }}">Create new</a>
{% endblock %}
