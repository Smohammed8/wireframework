{% extends 'base.html.twig' %}

{% block title %}AcademicSession{% endblock %}

{% block body %}
    <h1>AcademicSession</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ academic_session.id }}</td>
            </tr>
            <tr>
                <th>Year</th>
                <td>{{ academic_session.year }}</td>
            </tr>
            <tr>
                <th>Start</th>
                <td>{{ academic_session.start ? academic_session.start|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>End</th>
                <td>{{ academic_session.end ? academic_session.end|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_academic_session_index') }}">back to list</a>

    <a href="{{ path('app_academic_session_edit', {'id': academic_session.id}) }}">edit</a>

    {{ include('academic_session/_delete_form.html.twig') }}
{% endblock %}
